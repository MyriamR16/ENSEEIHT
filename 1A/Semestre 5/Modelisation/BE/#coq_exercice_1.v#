Require Import Naturelle.
Section Session1_2019_Logique_Exercice_1.

Variable A B C : Prop.

Theorem Exercice_1_Naturelle :  (A -> B -> C) -> ((A /\ B) -> C).
Proof.
I_imp H0.
I_imp H1.
E_imp(B).
E_imp(A).
Hyp H0.
E_et_g(B).
Hyp H1.
E_et_d(A).
Hyp H1.
Qed.

Theorem Exercice_1_Coq :   (A\/B)/\(~A) ->  B/\(~A).
Proof.
intros.
split.
elim H.
intros.

Qed.

End Session1_2019_Logique_Exercice_1.

